<h1>New story</h1>

<%= form_for(@story) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :category %><br>
    <%= f.text_field :category %>
  </div>

  <br/>

  <div class="actions">
    <%= f.submit %>
  </div>

<% end %>



<%= link_to 'Back', stories_path %>
